<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
	<cron>
		<url>/cron?store=ios</url>
		<description>Gather iOS store data</description>
		<schedule>every 12 hours from 10:00 to 22:00</schedule>
	</cron>
    <cron>
        <url>/cron?process=accounts</url>
        <description>Gather data account (linked account data)</description>
        <schedule>every 6 hours from 2:00 to 20:00</schedule>
    </cron>
    <cron>
        <url>/cron?tidy=item</url>
        <description>Tidy duplicate item rows</description>
        <schedule>every day 19:01</schedule>
    </cron>
    <cron>
        <url>/cron?process=itemproperties</url>
        <description>Get missing item properties</description>
        <schedule>every day 20:00</schedule>
    </cron>
</cronentries>