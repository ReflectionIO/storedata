<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  	<servlet>
		<servlet-name>Cron Tasks</servlet-name>
		<servlet-class>com.spacehopperstudios.storedatacollector.CronServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Dev Helper</servlet-name>
		<servlet-class>com.spacehopperstudios.storedatacollector.DevHelperServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Cron Tasks</servlet-name>
		<url-pattern>/gather</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Dev Helper</servlet-name>
		<url-pattern>/devhelper</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
</web-app>