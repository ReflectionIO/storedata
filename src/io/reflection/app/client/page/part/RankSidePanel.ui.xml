<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:c="urn:import:io.reflection.app.client.component">

	<ui:import field="io.reflection.app.client.res.Styles.STYLES_INSTANCE" />

	<ui:style gss="true">
	
	</ui:style>
	<g:HTMLPanel tag="form">

		<g:HTMLPanel styleName="{STYLES_INSTANCE.reflectionMainStyle.calendarContainer} {STYLES_INSTANCE.reflectionMainStyle.singleDateInput}">

			<div class="{STYLES_INSTANCE.reflectionMainStyle.dateSelectContainer}">
				<div class="{STYLES_INSTANCE.reflectionMainStyle.formField} {STYLES_INSTANCE.reflectionMainStyle.formFieldDateSelect}">
					<c:FormDateBox ui:field="dateBox"></c:FormDateBox>
					<span></span>
				</div>
			</div>

		</g:HTMLPanel>

		<c:FormFieldSelect ui:field="countryListBox" labelText="Country"></c:FormFieldSelect>

		<c:FormFieldSelect ui:field="appStoreListBox" labelText="App Store"></c:FormFieldSelect>

		<c:FormFieldSelect ui:field="categoryListBox" labelText="Category"></c:FormFieldSelect>

		<g:HTMLPanel ui:field="dailyDataRadio" styleName="{STYLES_INSTANCE.reflectionMainStyle.formFieldRadioList} {STYLES_INSTANCE.reflectionMainStyle.formFieldRadioListInline}">
			<ul>
				<li>
					<c:FormRadioButton ui:field="dailyDataRevenue" name="dailyData" label="Revenue" checked="true" />
				</li>
				<li>
					<c:FormRadioButton ui:field="dailyDataDownloads" name="dailyData" label="Downloads" />
				</li>
			</ul>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder> 