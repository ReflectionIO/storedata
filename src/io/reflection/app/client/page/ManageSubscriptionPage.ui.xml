				<section ui:field="currentStandard" class="{STYLES_INSTANCE.reflectionMainStyle.pricingOption} {STYLES_INSTANCE.reflectionMainStyle.pricingOptionStandard}">
					<header class="{STYLES_INSTANCE.reflectionMainStyle.pricingOptionHeader}">
						<h2 class="{STYLES_INSTANCE.reflectionMainStyle.pricingName}">Developer</h2>
						<div class="{STYLES_INSTANCE.reflectionMainStyle.pricingPriceContainer}">
							<span class="{STYLES_INSTANCE.reflectionMainStyle.pricingPrice}">FREE</span>
						</div>
						<span class="{STYLES_INSTANCE.reflectionMainStyle.packageFor}">For App Developers</span>
						<p class="{STYLES_INSTANCE.reflectionMainStyle.refIconBefore} {STYLES_INSTANCE.reflectionMainStyle.refIconBeforeCheck}">App Store Account Required</p>
					</header>
					<ul class="{STYLES_INSTANCE.reflectionMainStyle.pricingDetails}">
						<li>
							<span>Daily Top 200 App Revenue, Download and Rank Data</span>
							<a href="#" class="js-tooltip js-tooltip--right js-tooltip--right--no-pointer-padding whats-this-tooltip-icon-static" data-tooltip="View the latest revenue and download data for the games category in the UK, Germany, France &amp; Italy. More countries and categories will be available as the beta progresses">
								<span>What's this?</span>
							</a>
						</li>
						<li>
							<span>Leaderboard Rank History</span>
