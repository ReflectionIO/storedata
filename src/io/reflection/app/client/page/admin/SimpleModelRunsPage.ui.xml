<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cv="urn:import:com.google.gwt.user.cellview.client"
	xmlns:c="urn:import:io.reflection.app.client.component" xmlns:p="urn:import:io.reflection.app.client.part">
	<ui:import field="io.reflection.app.client.res.Styles.STYLES_INSTANCE" />
	<ui:style gss="true">
	
	</ui:style>
	<g:HTMLPanel styleName="{STYLES_INSTANCE.reflectionMainStyle.gridContainer}">
		<h3>Simple Model Run</h3>

		<div
			class="{STYLES_INSTANCE.reflectionMainStyle.grid__column} {STYLES_INSTANCE.reflectionMainStyle.grid__columnFullWidth} {STYLES_INSTANCE.reflectionMainStyle.filtersContainer}">
			<form>

				<g:HTMLPanel styleName="form-group">
					<label>Date</label>
					<c:DateSelector ui:field="dateSelector" />
				</g:HTMLPanel>

				<c:FormFieldSelect ui:field="listType" labelText="List Type"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="country" labelText="Country"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="appStore" labelText="App Store"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="category" labelText="Category"></c:FormFieldSelect>
			</form>
		</div>

		<cv:CellTable ui:field="simpleModelRunTable" styleName="{STYLES_INSTANCE.reflectionMainStyle.tableData}" />

		<p:SimplePager styleName="text-center" ui:field="simplePager" rangeLimited="true" />

	</g:HTMLPanel>
</ui:UiBinder> 