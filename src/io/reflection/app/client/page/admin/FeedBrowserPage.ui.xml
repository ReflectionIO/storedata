<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:io.reflection.app.client.part"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	.toolbar_spacing {
	    padding-top: 10px;
	    padding-bottom: 10px;
	}
	</ui:style>
	<g:HTMLPanel styleName="row"> <!-- CONTAINER-FLUID style -->

		<g:FormPanel styleName="col-md-2 well sidepanel">
			<g:HTMLPanel>

				<h2>
					Feed Browser
				</h2>
				<h5 class="text-muted">12-hourly feeds</h5>

				<g:HTMLPanel styleName="form-group">
					<label>Overview</label>
					<g:ListBox styleName="form-control" ui:field="mListType">
						<g:item value="all">All</g:item>
						<g:item value="paid">Paid</g:item>
						<g:item value="free">Free</g:item>
						<g:item value="grossing">Grossing</g:item>
					</g:ListBox>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="form-group">
					<label>Country</label>
					<g:ListBox styleName="form-control" ui:field="mCountry">
					</g:ListBox>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="form-group">
					<label>App Store</label>
					<g:ListBox styleName="form-control" ui:field="mAppStore">
					</g:ListBox>
				</g:HTMLPanel>

				<g:HTMLPanel styleName="form-group">
					<label>Category</label>
					<g:ListBox styleName="form-control" ui:field="category">
					</g:ListBox>
				</g:HTMLPanel>

			</g:HTMLPanel>
		</g:FormPanel>

		<g:HTMLPanel styleName="col-md-10">

			<p:Breadcrumbs ui:field="mBreadcrumbs" path="..." styleName="topPanel" />

			<c:CellTable ui:field="mFeeds" styleName="table table-hover table-striped" />

			<p:SimplePager styleName="text-center" ui:field="mPager" rangeLimited="true" />
			<g:HTMLPanel styleName="filler-bottom col-md-12 hidden-xs hidden-sm" />
		</g:HTMLPanel>


	</g:HTMLPanel>
</ui:UiBinder> 