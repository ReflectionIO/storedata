<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:io.reflection.app.client.part"
	xmlns:cv="urn:import:com.google.gwt.user.cellview.client" xmlns:c="urn:import:io.reflection.app.client.component">
	<ui:import field="io.reflection.app.client.res.Styles.STYLES_INSTANCE" />
	<ui:style gss="true">
	
	</ui:style>
	<g:HTMLPanel styleName="{STYLES_INSTANCE.reflectionMainStyle.gridContainer}">
				<h2>
					Feed Browser
				</h2>
		<div
			class="{STYLES_INSTANCE.reflectionMainStyle.grid__column} {STYLES_INSTANCE.reflectionMainStyle.grid__columnFullWidth} {STYLES_INSTANCE.reflectionMainStyle.filtersContainer}">
			<form>

				<h5 class="text-muted">12-hourly feeds</h5>

				<c:FormFieldSelect ui:field="mListType" labelText="List Type"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="mCountry" labelText="Country"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="mAppStore" labelText="App Store"></c:FormFieldSelect>

				<c:FormFieldSelect ui:field="category" labelText="Category"></c:FormFieldSelect>


			</form>
		</div>

			<p:Breadcrumbs ui:field="mBreadcrumbs" path="..." styleName="topPanel" />

		<cv:CellTable ui:field="mFeeds" styleName="{STYLES_INSTANCE.reflectionMainStyle.tableData}" />

			<p:SimplePager styleName="text-center" ui:field="mPager" rangeLimited="true" />


	</g:HTMLPanel>
</ui:UiBinder> 