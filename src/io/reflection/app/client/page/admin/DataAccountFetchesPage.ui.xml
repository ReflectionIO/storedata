<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:p="urn:import:io.reflection.app.client.part">
	<ui:import field="io.reflection.app.client.page.PageType.DataAccountsPageType" />
	<ui:style>
	.backLink {
	    margin-top: 20px;
	    margin-bottom: 20px;
	    margin-left: 25px;
	    display: inline-block;
	}
	</ui:style>
	<g:HTMLPanel>
		<div class="row">
			<g:HTMLPanel styleName="col-sm-10 col-sm-offset-1">
				<h2>Data Account fetches</h2>
				<g:InlineHyperlink ui:field="backLink" styleName="{style.backLink}" targetHistoryToken="{DataAccountsPageType.asTargetHistoryToken}"
					visible="false">&#8592;
					Back to data accounts</g:InlineHyperlink>
				<c:CellTable ui:field="dataAccountFetchTable" styleName="table table-hover table-striped" />

				<p:SimplePager styleName="text-center" ui:field="simplePager" rangeLimited="true" />

			</g:HTMLPanel>
			<g:HTMLPanel styleName="filler-bottom col-md-12 hidden-xs hidden-sm" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 