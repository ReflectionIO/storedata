<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:io.reflection.app.client.page.part"
	xmlns:part="urn:import:io.reflection.app.client.part" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style gss="true">
	
	</ui:style>
	<g:HTMLPanel styleName="row">

		<p:MyAccountSidePanel ui:field="myAccountSidePanel" />

		<g:HTMLPanel styleName="col-md-10">
			<g:HTMLPanel styleName="well well-sm well-title topPanel">
				<g:HTMLPanel styleName="pull-left">
					<h5>Notifications</h5>
				</g:HTMLPanel>
			</g:HTMLPanel>

			<part:Preloader ui:field="preloaderTable">
				<g:HTMLPanel ui:field="notificationsPanel">

					<div class="text-left">
						<g:Button ui:field="actions" styleName="btn btn-default text-center" visible="false">Actions</g:Button>
					</div>
					<c:CellTable ui:field="notificationsTable" styleName="table table-hover table-striped" />
					<part:SimplePager styleName="text-center" ui:field="simplePager" />

				</g:HTMLPanel>
			</part:Preloader>

		</g:HTMLPanel>
		<g:HTMLPanel styleName="filler-bottom col-md-12 hidden-xs hidden-sm" />
	</g:HTMLPanel>
</ui:UiBinder> 