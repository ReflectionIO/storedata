<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:import field="io.reflection.app.client.res.Styles.STYLES_INSTANCE" />
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="{STYLES_INSTANCE.reflectionMainStyle.formField} {STYLES_INSTANCE.reflectionMainStyle.isClosed}">
		<g:PasswordTextBox ui:field="passwordTextBox" styleName="" />
		<label ui:field="label">
			<div ui:field="strengthIndicatorPanel" class="{STYLES_INSTANCE.reflectionMainStyle.passwordStrengthIndicatorContainer}">
				<span ui:field="strengthDescription" class="{STYLES_INSTANCE.reflectionMainStyle.passwordStrengthDescription}">Strength</span>
				<div class="{STYLES_INSTANCE.reflectionMainStyle.passwordStrengthIndicator}">
					<span ui:field="strengthIndicator"></span>
				</div>
			</div>
		</label>
	</g:HTMLPanel>
</ui:UiBinder> 