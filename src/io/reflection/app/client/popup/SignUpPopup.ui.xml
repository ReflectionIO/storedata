			<g:Button ui:field="continueBrowsing" styleName="{STYLES_INSTANCE.reflectionMainStyle.refButtonLinkLarge}" text="Continue Browsing" />
		</g:HTMLPanel>

		<g:HTMLPanel ui:field="registerFormContainer" styleName="{STYLES_INSTANCE.reflectionMainStyle.registerFormContainer}">
			<h2 class="{STYLES_INSTANCE.reflectionMainStyle.headingStyleHeadingFive} {STYLES_INSTANCE.reflectionMainStyle.jsFadeContent}">Don't Miss Out, Register Now</h2>
			<p class="{STYLES_INSTANCE.reflectionMainStyle.jsFadeContent}">Business Accounts will be available on a first-come, first-served basis when our beta ends. Register your interest today to ensure you don’t miss
				out and we’ll notify you when it's ready.</p>
			<form class="{STYLES_INSTANCE.reflectionMainStyle.formsMidTheme}">
				<c:TextField ui:field="firstNameTextField" labelText="First Name" styleName="{STYLES_INSTANCE.reflectionMainStyle.jsRotateContent}" />
				<c:TextField ui:field="lastNameTextField" labelText="Last Name" styleName="{STYLES_INSTANCE.reflectionMainStyle.jsRotateContent}" />
				<c:TextField ui:field="companyTextField" labelText="Company" styleName="{STYLES_INSTANCE.reflectionMainStyle.jsRotateContent}" />
				<c:TextField ui:field="emailTextField" labelText="Email" styleName="{STYLES_INSTANCE.reflectionMainStyle.jsRotateContent}" />
				<div class="{STYLES_INSTANCE.reflectionMainStyle.generalErrorMessage}">
					<p ui:field="generalErrorParagraph"></p>
				</div>
				<c:LoadingButton ui:field="registerInterest" styleName="{STYLES_INSTANCE.reflectionMainStyle.refButtonFullWidth} {STYLES_INSTANCE.reflectionMainStyle.jsRotateContent}">Submit</c:LoadingButton>
			</form>
