<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='images' type='io.reflection.app.client.res.Images' />
	<ui:style>
	.reminder {
		padding-top: 50px;
		padding-bottom: 100px;
	}
	</ui:style>
	<g:HTMLPanel styleName="text-center col-sm-12 {style.reminder}"
		visible="false">
		<g:Image resource="{images.emailSent}" />
		<h4 style="margin-top: 20px; margin-bottom: 20px;">Reminder on it's way</h4>
		<p>We've sent a reminder email to the address you specified, it
			should
			be with you in a few moments</p>
	</g:HTMLPanel>
</ui:UiBinder> 