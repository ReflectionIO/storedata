<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style gss="true">
	.breadcrumb {
		padding: 8px 15px;
		margin-bottom: 20px;
		list-style: none;
		background-color: #f5f5f5;
		border-radius: 4px;
		margin-top: 0;
	}
	
	.breadcrumb li {
		display: inline-block;
	}
	
	.breadcrumb>li+li:before {
		content: "/\00a0";
		padding: 0 5px;
		color: #666;
	}
	</ui:style>
	<g:HTMLPanel>
		<ol ui:field="mList" class="{style.breadcrumb}">
		</ol>
	</g:HTMLPanel>
</ui:UiBinder> 