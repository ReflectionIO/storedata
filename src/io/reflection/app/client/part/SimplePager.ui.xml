<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:import field="io.reflection.app.client.res.Styles.STYLES_INSTANCE" />
	<ui:style gss="true">
	
	</ui:style>
	<g:HTMLPanel styleName="{STYLES_INSTANCE.reflectionMainStyle.pagination}">
		<g:Anchor ui:field="mFirstPage" href="#" styleName="{STYLES_INSTANCE.reflectionMainStyle.pagesLink}">
			First
		</g:Anchor>
		<g:Anchor ui:field="mPrevPage" href="#" styleName="{STYLES_INSTANCE.reflectionMainStyle.pagesLink}">
			Previous
		</g:Anchor>

		<ul ui:field="mList" style="display: none">
			<li ui:field="mFirstPageItem" class="disabled">
			</li>
			<li ui:field="mPrevPageItem" class="disabled">
			</li>
			<li>
				<span ui:field="mLabel" />
			</li>
			<li ui:field="mNextPageItem" class="disabled">
			</li>
			<li ui:field="mFastForwardItem" class="disabled">
				<g:Anchor ui:field="mFastForward">
					FF
				</g:Anchor>
			</li>
			<li ui:field="mLastPageItem" class="disabled">
				<g:Anchor ui:field="mLastPage">
					Last
				</g:Anchor>
			</li>
		</ul>

		<g:Anchor ui:field="mNextPage" href="#" styleName="{STYLES_INSTANCE.reflectionMainStyle.pagesLink}">
			Next
		</g:Anchor>
	</g:HTMLPanel>
</ui:UiBinder> 