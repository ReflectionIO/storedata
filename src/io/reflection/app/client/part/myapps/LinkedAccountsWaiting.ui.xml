<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='images' type='io.reflection.app.client.res.Images' />
	<ui:style>
	.partStyle {
		margin-top: 50px;
		margin-bottom: 60px;
	}
	</ui:style>
	<g:HTMLPanel>

		<g:HTMLPanel styleName="well well-sm col-md-12">

			<h5 class="col-md-2">Overview</h5>
		</g:HTMLPanel>

		<g:HTMLPanel styleName="col-md-12 text-center {style.partStyle}">
			<g:Image resource="{images.clock}" />
			<h1 style="margin-top: 20px; margin-bottom: 30px;">Won't be a moment...</h1>
			<h4> We're fetching your app information which may take a few
				moments.</h4>
			<h4>We'll send you an email when they are ready to view.</h4>
		</g:HTMLPanel>

	</g:HTMLPanel>
</ui:UiBinder> 