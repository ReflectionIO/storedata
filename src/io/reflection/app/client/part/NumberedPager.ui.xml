<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:a="urn:import:io.reflection.app.client.part">
	<ui:style>
		.compensateTop {
		top: 1px;
		}

		.pagination {
		display: inline-block;
		padding-left: 0;
		margin: 20px 0;
		border-radius: 4px;
		}

		.pagination li
		{
		display:inline;
		}
	</ui:style>
	<g:HTMLPanel ui:field="htmlPanel">
		<g:HTMLPanel ui:field="mFirstPageItem">
				<a:MyAnchor ui:field="mFirstPage">
					first
				</a:MyAnchor>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="mPrevPageItem">
				<a:MyAnchor ui:field="mPrevPage">
					previous
				</a:MyAnchor>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="mNextPageItem">
				<g:Anchor ui:field="mNextPage">
					next
				</g:Anchor>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="mLastPageItem">
				<g:Anchor ui:field="mLastPage">
					last
				</g:Anchor>
			</g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 