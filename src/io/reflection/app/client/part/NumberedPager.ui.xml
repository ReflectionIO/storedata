<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:io.reflection.app.client.part">
	<ui:style gss="true">
		.compensateTop {
		top: 1px;
		}

		.pagination {
		display: inline-block;
		padding-left: 0;
		margin: 20px 0;
		border-radius: 4px;
		}

		.pagination li
		{
		display:inline;
		}

		.spaceRightGap {
		margin-right:12px;
		}

		
    .headerDivider {
     border-left:1px solid #d2d2d2; 
     margin-right:12px;
}
  
	</ui:style>
	<g:HTMLPanel ui:field="htmlPanel">
		<a:MyAnchor ui:field="mFirstPage" styleName="{style.spaceRightGap}">
			First
		</a:MyAnchor>
		<g:InlineLabel ui:field="firstBar" styleName="{style.headerDivider}"></g:InlineLabel>
		<a:MyAnchor ui:field="mPrevPage" styleName="{style.spaceRightGap}">
			Prev
		</a:MyAnchor>
		<a:MyAnchor ui:field="mNextPage" addStyleNames="{style.spaceRightGap}">
			Next
		</a:MyAnchor>
		<g:InlineLabel ui:field="lastBar" styleName="{style.headerDivider}"></g:InlineLabel>
		<a:MyAnchor ui:field="mLastPage" styleName="{style.spaceRightGap}">
			Last
		</a:MyAnchor>
	</g:HTMLPanel>
</ui:UiBinder> 