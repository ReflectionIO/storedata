<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:a="urn:import:io.reflection.app.client.part">
	<ui:style>
		.compensateTop {
		top: 1px;
		}

		.pagination {
		display: inline-block;
		padding-left: 0;
		margin: 20px 0;
		border-radius: 4px;
		}

		.pagination li
		{
		display:inline;
		}

        .spaceApart{
            margin-right:3px;
        }
	</ui:style>
	<g:HTMLPanel ui:field="htmlPanel">
				<a:MyAnchor ui:field="mFirstPage" styleName="{style.spaceApart}">
					First
				</a:MyAnchor>
				<g:InlineLabel ui:field="firstBar" styleName="{style.spaceApart}">|</g:InlineLabel>
				<a:MyAnchor ui:field="mPrevPage" styleName="{style.spaceApart}">
					Prev
				</a:MyAnchor>
				<a:MyAnchor ui:field="mNextPage" styleName="{style.spaceApart}">
					Next
				</a:MyAnchor>
				<g:InlineLabel ui:field="lastBar" styleName="{style.spaceApart}">|</g:InlineLabel>
				<a:MyAnchor ui:field="mLastPage" styleName="{style.spaceApart}">
					Last
				</a:MyAnchor>
	</g:HTMLPanel>
</ui:UiBinder> 