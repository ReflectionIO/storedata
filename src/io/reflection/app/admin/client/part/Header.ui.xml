<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with field='res' type='io.reflection.app.admin.client.res.Images'/>
    <ui:style>
        .header {
	       background: #272833;
        }
        
        .aligntoimage {
			vertical-align: bottom;
		}
    </ui:style>
 <g:HTMLPanel ui:field="pnlHeader" styleName="navbar navbar-inverse navbar-fixed-top {style.header}">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href=""><g:Image styleName="{style.aligntoimage}" resource="{res.logo}"/>&nbsp;&nbsp;Reflection</a>
            </div>
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav" ui:field="mNavList">
                    <li ui:field="mRanksItem">
                        <g:InlineHyperlink ui:field="mRanksLink" targetHistoryToken="ranks">Ranks</g:InlineHyperlink>
                    </li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </g:HTMLPanel>
</ui:UiBinder> 